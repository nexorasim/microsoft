{
  "name": "myanmar-esim-enterprise-portal",
  "version": "1.0.0",
  "description": "Premium-grade Myanmar eSIM Enterprise Management Portal with comprehensive security, RBAC, and multi-carrier support",
  "main": "index.html",
  "scripts": {
    "dev": "live-server --port=3000 --open=index.html --cors",
    "build": "npm run validate && npm run minify && npm run optimize",
    "validate": "npm run lint && npm run test && npm run security-check",
    "lint": "eslint src/**/*.js --fix",
    "test": "jest --coverage --passWithNoTests",
    "security-check": "npm audit --audit-level moderate",
    "minify": "npm run minify-css && npm run minify-js",
    "minify-css": "cleancss src/styles/premium.css -o dist/premium.min.css",
    "minify-js": "terser src/scripts/*.js src/api/*.js -o dist/bundle.min.js --compress --mangle",
    "optimize": "npm run optimize-images && npm run generate-sitemap",
    "optimize-images": "imagemin assets/**/*.{jpg,png,svg} --out-dir=dist/assets/",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "deploy": "npm run build && npm run deploy-vercel && npm run deploy-azure",
    "deploy-vercel": "vercel --prod",
    "deploy-azure": "az webapp deployment source config-zip",
    "deploy-cloudflare": "wrangler publish",
    "start": "npm run dev",
    "clean": "rimraf dist/ && rimraf coverage/",
    "format": "prettier --write \"**/*.{js,css,html,json,md}\"",
    "precommit": "npm run validate && npm run format"
  },
  "keywords": [
    "esim",
    "enterprise",
    "myanmar",
    "mpt",
    "atom",
    "mytel",
    "u9",
    "management",
    "portal",
    "security",
    "rbac",
    "premium"
  ],
  "author": {
    "name": "NexoraSIM Enterprise",
    "email": "enterprise@nexorasim.com",
    "url": "https://nexorasim.com"
  },
  "license": "Proprietary",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/nexorasim/microsoft.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "qrcode": "^1.5.3",
    "chart.js": "^4.4.0",
    "date-fns": "^2.30.0",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "live-server": "^1.2.2",
    "clean-css-cli": "^5.6.2",
    "terser": "^5.24.0",
    "imagemin": "^8.0.1",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^9.0.2",
    "imagemin-svgo": "^10.0.1",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "jest": "^29.7.0",
    "rimraf": "^5.0.5",
    "@vercel/cli": "^32.5.0",
    "wrangler": "^3.15.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "security": {
    "csp": {
      "default-src": "'self'",
      "script-src": "'self' 'unsafe-inline'",
      "style-src": "'self' 'unsafe-inline' https://fonts.googleapis.com",
      "font-src": "'self' https://fonts.gstatic.com",
      "img-src": "'self' data:",
      "connect-src": "'self'",
      "frame-ancestors": "'none'",
      "base-uri": "'self'",
      "form-action": "'self'"
    },
    "headers": {
      "Strict-Transport-Security": "max-age=31536000; includeSubDomains; preload",
      "X-Frame-Options": "DENY",
      "X-Content-Type-Options": "nosniff",
      "Referrer-Policy": "strict-origin-when-cross-origin",
      "Permissions-Policy": "geolocation=(), microphone=(), camera=()"
    }
  },
  "performance": {
    "budgets": [
      {
        "type": "initial",
        "maximumWarning": "300kb",
        "maximumError": "500kb"
      }
    ]
  },
  "accessibility": {
    "wcag": "AA",
    "standards": ["WCAG 2.1", "Section 508"]
  },
  "seo": {
    "title": "Myanmar eSIM Enterprise Portal",
    "description": "Premium enterprise eSIM management platform for Myanmar carriers",
    "keywords": "eSIM, enterprise, Myanmar, MPT, ATOM, MYTEL, U9",
    "robots": "noindex, nofollow"
  },
  "deployment": {
    "environments": {
      "development": {
        "url": "http://localhost:3000",
        "debug": true
      },
      "staging": {
        "url": "https://staging-myanmar-esim.vercel.app",
        "debug": false
      },
      "production": {
        "url": "https://myanmar-esim-enterprise.com",
        "debug": false
      }
    },
    "platforms": {
      "vercel": {
        "enabled": true,
        "config": "vercel.json"
      },
      "azure": {
        "enabled": true,
        "config": "azure-pipelines.yml"
      },
      "cloudflare": {
        "enabled": true,
        "config": "wrangler.toml"
      }
    }
  },
  "monitoring": {
    "analytics": false,
    "performance": true,
    "security": true,
    "uptime": true
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["eslint:recommended"],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "no-console": "warn",
      "no-unused-vars": "error",
      "prefer-const": "error",
      "no-var": "error"
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2
  }
}